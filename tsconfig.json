{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"baseUrl": ".",
		"paths": {
			"@/*": ["src/*"],
			"@components/*": ["src/components/*"],
			"@screens/*": ["src/screens/*"],
			"@contexts/*": ["src/contexts/*"],
			"@services/*": ["src/services/*"],
			"@types/*": ["src/types/*"],
			"@utils/*": ["src/utils/*"],
			"@styles/*": ["src/styles/*"]
		}
	}
}