{
  "name": "zumi-mobile",
  "version": "1.0.7",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:android": "eas build --platform android --profile production --local",
    "build:android:apk": "cd android && .\\gradlew.bat assembleRelease",
    "build:android:cloud": "eas build --platform android --profile production",
    "build:ios:cloud": "eas build --platform ios --profile production",
    "postinstall": "patch-package",
    "postprebuild": "node scripts/copy-splash.js",
    "install:remote": "adb install -r android/app/build/outputs/apk/release/app-release.apk",
    "install:icon": "node scripts/install-launcher-icon.js"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-navigation/bottom-tabs": "^7.8.8",
    "@react-navigation/native": "^7.1.22",
    "@react-navigation/stack": "^7.6.8",
    "axios": "^1.13.2",
    "expo": "~54.0.25",
    "expo-application": "~7.0.7",
    "expo-av": "^16.0.7",
    "expo-constants": "~18.0.10",
    "expo-crypto": "^15.0.7",
    "expo-document-picker": "^14.0.7",
    "expo-file-system": "~19.0.19",
    "expo-image-manipulator": "~14.0.7",
    "expo-image-picker": "^17.0.8",
    "expo-intent-launcher": "~13.0.7",
    "expo-linear-gradient": "^15.0.7",
    "expo-linking": "^8.0.11",
    "expo-notifications": "~0.32.13",
    "expo-secure-store": "^15.0.7",
    "expo-sensors": "^15.0.7",
    "expo-splash-screen": "^31.0.11",
    "expo-status-bar": "~3.0.8",
    "expo-web-browser": "^15.0.9",
    "react": "^19.1.0",
    "react-native": "^0.81.5",
    "react-native-bluetooth-classic": "^1.73.0-rc.17",
    "react-native-gesture-handler": "^2.29.1",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-track-player": "4.1.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@types/react": "~19.1.0",
    "@types/react-native": "^0.72.8",
    "babel-preset-expo": "^54.0.7",
    "patch-package": "^8.0.1",
    "typescript": "~5.9.2"
  },
  "private": true
}
