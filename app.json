{
  "expo": {
    "name": "Zumi Music",
    "slug": "zumi-mobile",
    "version": "1.0.7",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#6e254a"
    },
    "jsEngine": "hermes",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "fr.phearion.zumi",
      "splash": {
        "image": "./assets/splash-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#6e254a"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#6e254a"
      },
      "package": "fr.phearion.zumi",
      "permissions": [
        "REQUEST_INSTALL_PACKAGES",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
        "FOREGROUND_SERVICE",
        "FOREGROUND_SERVICE_MEDIA_PLAYBACK",
        "POST_NOTIFICATIONS",
        "WAKE_LOCK"
      ],
      "splash": {
        "image": "./assets/splash-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#6e254a",
        "mdpi": "./assets/splash-icon.png",
        "hdpi": "./assets/splash-icon.png",
        "xhdpi": "./assets/splash-icon.png",
        "xxhdpi": "./assets/splash-icon.png",
        "xxxhdpi": "./assets/splash-icon.png"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "zumi",
    "plugins": [
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff"
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#6e254a",
          "image": "./assets/splash-icon.png",
          "imageWidth": 200
        }
      ]
    ],
    "extra": {
      "apiUrl": "https://api.international.phearion.fr/zumi",
      "deepLinkScheme": "zumi",
      "EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID": "298755969973-nnn4qpsjc28fifmt2ea75o3ppnrua3na.apps.googleusercontent.com",
      "EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID": "298755969973-r58meo1o2sagtv42cusicv186rgumd8r.apps.googleusercontent.com",
      "EXPO_PUBLIC_GOOGLE_EXPO_CLIENT_ID": "298755969973-nnn4qpsjc28fifmt2ea75o3ppnrua3na.apps.googleusercontent.com",
      "eas": {
        "projectId": "5e5bd45b-1df0-4336-b351-f8d7a14f8e7b"
      }
    },
    "owner": "phanthive"
  }
}
